      "mentions": []
    },
    {
      "id": "1141142142690005184",
      "type": "Default",
      "timestamp": "2023-08-16T07:51:28.213+09:00",
      "timestampEdited": null,
      "callEndedTimestamp": null,
      "isPinned": false,
      "content": "Fat gremlin just doesn't reward a dedicated deck the same way the others do,and this is dead outside a dedicated deck",
      "author": {
        "id": "160947148249235456",
        "name": "_brickman",
        "discriminator": "0000",
        "nickname": "Brickman",
        "color": null,
        "isBot": false,
        "roles": [],
        "avatarUrl": "https://cdn.discordapp.com/avatars/160947148249235456/7a27bca25d80e014761a67ab5f86fd5b.png?size=512"
      },
      "attachments": [],
