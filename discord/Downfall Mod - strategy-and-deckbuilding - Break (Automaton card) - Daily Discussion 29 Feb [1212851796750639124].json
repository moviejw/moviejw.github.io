        "name": "lxixcdxx",
        "discriminator": "0000",
        "nickname": "d/dx(6x^70)",
        "color": "#6F8D90",
        "isBot": false,
        "roles": [
          {
            "id": "793945427044859945",
            "name": "Moderator",
