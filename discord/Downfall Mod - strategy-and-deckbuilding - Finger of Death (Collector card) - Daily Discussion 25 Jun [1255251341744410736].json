    {
      "id": "1255271239090569328",
      "type": "Default",
      "timestamp": "2024-06-26T06:19:45.211+09:00",
      "timestampEdited": null,
      "callEndedTimestamp": null,
      "isPinned": false,
      "content": "It costs less, doesn't clog up your draw, can be reliably played on the first turn of combat with some setup and all it asks of you in return is to keep the enemy afflicted",
      "author": {
        "id": "206194901438824448",
        "name": "gangur",
        "discriminator": "0000",
        "nickname": "Gangur",
        "color": null,
        "isBot": false,
        "roles": [],
