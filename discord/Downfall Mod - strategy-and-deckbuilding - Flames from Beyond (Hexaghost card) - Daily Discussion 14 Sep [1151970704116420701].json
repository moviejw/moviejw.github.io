    {
      "id": "1152091291916718101",
      "type": "Default",
      "timestamp": "2023-09-15T12:59:28.814+09:00",
      "timestampEdited": null,
      "callEndedTimestamp": null,
      "isPinned": false,
      "content": "flames from beyond: “lookin’ good hexaghost” “aww you’re sweet”\n\nstone calendar: “lookin’ good hexaghost” “HELLO HUMAN RESOURCES???”",
      "author": {
        "id": "125461462276636672",
        "name": "occultpyromancer",
        "discriminator": "0000",
        "nickname": "occultpyromancer, briar patcher",
        "color": null,
        "isBot": false,
        "roles": [],
        "avatarUrl": "https://cdn.discordapp.com/avatars/125461462276636672/a2b60458f09ed817b4e79feaec0e1a21.png?size=512"
      },
      "attachments": [],
      "embeds": [],
