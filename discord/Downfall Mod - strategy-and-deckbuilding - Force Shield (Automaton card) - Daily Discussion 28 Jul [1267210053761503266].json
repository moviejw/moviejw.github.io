      "id": "1267252567931621376",
      "type": "Default",
      "timestamp": "2024-07-29T07:49:16.609+09:00",
      "timestampEdited": null,
      "callEndedTimestamp": null,
      "isPinned": false,
      "content": "similar to overheat, a 5/10 card on a class with an average of 7.5/10",
      "author": {
        "id": "417469040135634944",
        "name": "nic200",
        "discriminator": "0000",
        "nickname": "Nic200",
        "color": null,
