      "content": "would it be good if it was 4",
      "author": {
        "id": "484682433925611542",
        "name": "dark_brick",
        "discriminator": "0000",
        "nickname": "Dark_Brick",
        "color": null,
        "isBot": false,
        "roles": [],
        "avatarUrl": "https://cdn.discordapp.com/avatars/484682433925611542/a1ce6a4544e638258200f569829dd6ed.png?size=512"
