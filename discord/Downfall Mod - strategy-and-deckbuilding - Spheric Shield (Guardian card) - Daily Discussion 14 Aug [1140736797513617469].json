    {
      "id": "1140753259481551038",
      "type": "Default",
      "timestamp": "2023-08-15T06:06:11.231+09:00",
      "timestampEdited": null,
      "callEndedTimestamp": null,
      "isPinned": false,
      "content": "This card makes it way easier to do turbo blur, though you still can't keep it up forever",
      "author": {
        "id": "160947148249235456",
        "name": "_brickman",
        "discriminator": "0000",
        "nickname": "Brickman",
        "color": null,
        "isBot": false,
        "roles": [],
        "avatarUrl": "https://cdn.discordapp.com/avatars/160947148249235456/7a27bca25d80e014761a67ab5f86fd5b.png?size=512"
