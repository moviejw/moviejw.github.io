    {
      "id": "1175166652325838948",
      "type": "Default",
      "timestamp": "2023-11-18T05:12:43.455+09:00",
      "timestampEdited": null,
      "callEndedTimestamp": null,
      "isPinned": false,
      "content": "Champ is a very interesting character because you are forced to take cards that don't do the thing you want to naturally do, and strength/strikes are one of the easier pivots.",
      "author": {
        "id": "370020173610090537",
        "name": "blueguy_",
        "discriminator": "0000",
        "nickname": "Blue Guy",
        "color": null,
        "isBot": false,
        "roles": [],
