    {
      "id": "1221606398039687291",
      "type": "Default",
      "timestamp": "2024-03-25T08:47:41.642+09:00",
      "timestampEdited": "2024-03-25T08:48:11.76+09:00",
      "callEndedTimestamp": null,
      "isPinned": false,
      "content": "this also has a weird upgrade that I don't value to highly (its fine for hallway fights; but if you want to use the command your losing the slimes you want to keep)",
      "author": {
        "id": "826258620513648660",
        "name": "ahoodedman",
        "discriminator": "0000",
        "nickname": "ahoodedman",
        "color": null,
        "isBot": false,
        "roles": [],
        "avatarUrl": "https://cdn.discordapp.com/avatars/826258620513648660/54cf424fe819e05bdeb245afa0d88dba.png?size=512"
      },
