    {
      "id": "1058487861227290694",
      "type": "Default",
      "timestamp": "2022-12-31T05:52:51.87+09:00",
      "timestampEdited": null,
      "callEndedTimestamp": null,
      "isPinned": false,
      "content": "Honestly it's gotta be the very worst relic in downfall (unless you count ones with outright detrimental effects). If you ever drop down to 1 hp, it's because the run isn't working out. This does nothing to actually help, it just delays your misery.",
      "author": {
        "id": "417469040135634944",
        "name": "nic200",
        "discriminator": "0000",
        "nickname": "Nic200",
        "color": null,
        "isBot": false,
        "roles": [],
        "avatarUrl": "https://cdn.discordapp.com/avatars/417469040135634944/c1dc7f54fb565fcc38eaf9e60751fe5f.png?size=512"
