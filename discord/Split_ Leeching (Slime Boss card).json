    {
      "id": "1230661803491721236",
      "type": "Reply",
      "timestamp": "2024-04-19T08:30:38.543+09:00",
      "timestampEdited": null,
      "callEndedTimestamp": null,
      "isPinned": false,
      "content": "I acutally dislike upgrading glop chop in some decks because it cannot make leeching slime anymore! Plus slime boss has a lot of high value upgrades, much more then your average character (lick upgrades plus lots of power and card draw upgrades to take)",
      "author": {
        "id": "826258620513648660",
        "name": "ahoodedman",
        "discriminator": "0000",
        "nickname": "ahoodedman",
        "color": null,
        "isBot": false,
        "roles": [],
        "avatarUrl": "https://cdn.discordapp.com/avatars/826258620513648660/54cf424fe819e05bdeb245afa0d88dba.png?size=512"
      },
      "attachments": [],
